{
  "name": "dog-ranch",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "node --loader ts-node/esm server.ts",
    "build": "bun run build:client && bun run build:server",
    "build:client": "bunx vite build --outDir dist/client",
    "build:server": "bunx vite build --ssr src/entry-server.ts --outDir dist/server",
    "preview": "NODE_ENV=production node --loader ts-node/esm server.ts server.ts",
    "start": "NODE_ENV=production node --loader ts-node/esm server.ts server.ts",
    "vercel-build": "bun run build"
  },
  "dependencies": {
    "@devwareng/vanilla-ts": "^1.2.134",
    "animate.css": "^4.1.1",
    "compression": "^1.8.1",
    "express": "^5.1.0",
    "sirv": "^3.0.2",
    "zod": "^4.1.5"
  },
  "devDependencies": {
    "@tailwindcss/vite": "^4.1.13",
    "@types/compression": "^1.8.1",
    "@types/express": "^5.0.3",
    "@types/node": "^24.3.1",
    "boxicons": "^2.1.4",
    "rollup": "3",
    "tailwindcss": "^4.1.13",
    "ts-node": "^10.9.2",
    "typescript": "~5.9.2",
    "vite": "^5",
    "vite-plugin-checker": "^0.10.3"
  },
  "trustedDependencies": [
    "@tailwindcss/oxide",
    "esbuild"
  ]
}