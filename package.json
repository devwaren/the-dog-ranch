    "sirv": "^3.0.2",
    "zod": "^4.1.5"
  },
  "devDependencies": {
    "@tailwindcss/vite": "^4.1.13",
    "@types/express": "^5.0.3",
    "@types/node": "^24.3.1",
    "boxicons": "^2.1.4",
    "cross-env": "^10.0.0",
    "tailwindcss": "^4.1.13",
    "tsx": "^4.20.5",
    "typescript": "~5.9.2",
    "vite": "^7.1.5",
